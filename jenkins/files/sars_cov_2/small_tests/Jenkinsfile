                                        string(name: 'SEQUENCING_TECHNOLOGY', value: "illumina"),
                                        string(name: 'KIT', value: "ARTIC_V4.1"),
                                ]
                            )
                        }
                    }
                }
                stage('ont/v4.1') {
                    steps {
                        script {
                            build (
                                job: "${PSGA_PIPELINE}/${env.BRANCH_NAME}",
                                parameters: [
                                        booleanParam(name: 'VALIDATION_TEST', value: true),
                                        string(name: 'METADATA', value: "s3://synthetic-data-dev/UKHSA/requirements/CL0015b/ont_v4_1_input/metadata.csv"),
